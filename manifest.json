      "image": "arrow.triangle.merge"
    },
    {
      "identifier": "preferences",
      "image": "doc.badge.gearshape.fill"
    }
  ],
  "libraries": [
    {
      "identifier": "noteToSubtasksLib"
    },
    {
      "identifier": "noteToSubtasksConfig"
    }
  ]
}